{
    "builds": [
      {
        "src": "TodoList/wsgi.py",
        "use": "@vercel/python",
        "config": {"runtime":"python3.12"}
      },
      {
        "src": "buildfiles.sh",
        "use": "@vercel/static_build",
        "config": {"distDir":"staticfiles_build"}
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "TodoList/wsgi.py"
      }
    ]
}
